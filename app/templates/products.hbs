<h2>List of products</h2>

{{log model}}
{{#each model as |p|}}
  <div class="product col-md-3">
    <p>{{p.productName}}</p>
    <img src="/productImages/{{p.productImageName}}" alt="image for {{p.productName}}" />
    <br /><br />
    <button class="btn btn-primary" {{action "addToCart" p}}>Add to cart</button>
  </div>
{{/each}}
